{
  "name": "mdonef/collections",
  "type": "library",
  "description": "Collections library with focus on immutability and functional approach",
  "license": "MIT",
  "authors": [
    {
      "name": "Milan Donef",
      "email": "info@donef.cz"
    }
  ],
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/mdonef/phpstan-collections.git"
    }
  ],
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/mdonef/phpstan-collections.git"
    }
  ],
  "require": {
    "php": ">=8.0",
    "ext-json": "*"
  },
  "require-dev": {
    "ergebnis/composer-normalize": "^2.0.2",
    "phpstan/phpstan": "^2.0",
    "phpunit/phpunit": "^9.4.2",
    "slevomat/coding-standard": "^6.4.1",
    "squizlabs/php_codesniffer": "^3.5.0",
    "mdonef/phpstan-collections": "^0.2"
  },
  "config": {
    "bin-dir": "bin",
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "ergebnis/composer-normalize": true
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.5.x-dev"
    }
  },
  "autoload": {
    "psr-4": {
      "": [
        "src",
        "tests"
      ]
    },
    "files": [
      "src/Bonami/Collection/helpers.php"
    ]
  },
  "autoload-dev": {
    "files": [
      "tests/Bonami/Collection/helpers.php"
    ]
  }
}
